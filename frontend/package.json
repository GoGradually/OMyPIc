{
  "name": "omypic-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "preview": "vite preview",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "ws": "^8.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "electron": "^30.0.9",
    "electron-builder": "^24.13.3",
    "vite": "^5.4.3",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.omypic.app",
    "files": [
      "dist/**",
      "electron/**"
    ],
    "directories": {
      "buildResources": "electron"
    }
  }
}
